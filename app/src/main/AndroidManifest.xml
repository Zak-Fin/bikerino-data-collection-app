<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">
    <uses-permission android:name="android.permission.MANAGE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <!--    <uses-permission android:name="android.permission.WAKE_LOCK" />-->
    <application
        android:allowBackup="true"
        android:icon="@mipmap/bikerino_dev"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/bikerino_dev_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BikerinoPrototype">
        <activity android:name="com.example.bikerino_data_collection_app.MainActivity" android:screenOrientation="portrait">>

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>